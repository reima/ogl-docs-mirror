    <refnamediv>
        <refname>determinant</refname>
        <refpurpose>calculate the determinant of a matrix</refpurpose>
    </refnamediv>
    <refsynopsisdiv><title>Declaration</title>
        <funcsynopsis>
            <funcprototype>
                <funcdef>float <function>determinant</function></funcdef>
                <paramdef>mat2 <parameter>m</parameter></paramdef>
            </funcprototype>
            <funcprototype>
                <funcdef>float <function>determinant</function></funcdef>
                <paramdef>mat3 <parameter>m</parameter></paramdef>
            </funcprototype>
            <funcprototype>
                <funcdef>float <function>determinant</function></funcdef>
                <paramdef>mat4 <parameter>m</parameter></paramdef>
            </funcprototype>
            <funcprototype>
                <funcdef>double <function>determinant</function></funcdef>
